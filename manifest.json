{
  "manifest_version": 2,
  "name": "Simple adblock",
  "description": "Simple adblock",
  "version": "0.1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "96": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
