{
  "manifest_version": 2,
  "name": "Simple adblock",
  "description": "Simple adblock",
  "version": "0.1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "96": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Simple adblock"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "web_accessible_resources": [
    "on.png",
    "off.png"
  ]
}
